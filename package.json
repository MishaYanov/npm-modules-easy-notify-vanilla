{
  "name": "easy-notify-vanilla",
  "private": false,
  "version": "0.2.7",
  "keywords": [
    "notify",
    "notification",
    "toast",
    "alert",
    "message",
    "vanilla"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "compile": "tsc",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^2.1.0"
  },
  "dependencies": {
    "sass": "^1.71.0"
  }
}
